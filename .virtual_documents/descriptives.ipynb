wcgs |>
  estat(~ chol|chd) |>
  as_hux() |> theme_pubh() |> set_width(0.8) |> 
  add_footnote("Data from the WCGS.", font_size = 9)








wcgs |>
  group_by(chd) |>
  select(chol) |>
