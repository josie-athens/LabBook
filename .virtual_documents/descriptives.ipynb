    Median = median(chol, na.rm = TRUE),
    CV = rel_dis(chol)
  ) 











wcgs |>
  data_summary( 
    n = n(),
    Mean = mean(chol,  na.rm = TRUE), 
    SD = sd(chol,  na.rm = TRUE),
    CV = rel_dis(chol),
    by = c("chd", "smoker"),
    include_na = FALSE
  )


